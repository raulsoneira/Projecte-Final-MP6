{
  "name": "tailwind-parcel-project",
  "version": "1.0.0",
  "description": "Projecte que utilitza Tailwind CSS i Parcel",
  "scripts": {
    "dev": "parcel src/index.html && parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "optimitzar_imatge": "node optimitzar_imatge.js",
    "dev-with-images": "npm run optimitzar_imatge && npm run dev",
    "build-with-images": "npm run optimitzar_imatge && npm run build"
  },
  "keywords": [
    "tailwind",
    "parcel",
    "css",
    "html",
    "javascript"
  ],
  "author": "El teu nom",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-postcss": "^2.10.0",
    "autoprefixer": "^10.4.16",
    "parcel": "^2.12.0",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.3"
  },
  "dependencies": {
    "flowbite": "^2.3.0",
    "install": "^0.13.0",
    "npm": "^10.5.2",
    "sharp": "^0.33.3",
    "simple-parallax-js": "^5.6.2"
  }
}
